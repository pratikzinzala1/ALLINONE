        <item name="colorPrimary">@color/basilgreen800</item>
        <item name="colorPrimaryVariant">@color/basilgreen500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/basilorange800</item>
        <item name="colorSecondaryVariant">@color/basilorange500</item>
        <item name="colorOnSecondary">@color/black</item>
